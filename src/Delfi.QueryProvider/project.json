{
  "name": "Delfi.QueryProvider",
  "title": "Delfi QueryProvider",
  "authors": [ "Temitope Ajileye" ],
  "language": "en-GB",
  "copyright": "MIT",
  "brief": "Graph Database Query Provider",
  "description": "Graph Database Query Provider",
  "version": "0.2.0",
  "versions": [
    {
      "Version": "0.2.0",
      "Description": "Initial version"
    }
  ],

  "dependencies": {
    "Microsoft.Extensions.Configuration": "1.0.0",
    "Microsoft.Extensions.Configuration.Binder": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "NETStandard.Library": "1.5.0-rc2-24027",
    "Newtonsoft.Json": "9.0.1",
    "Sparql.Algebra": "0.2.10",
    "System.Collections.Specialized": "4.0.1"
  },

  "frameworks": {
    "netstandard1.5": {
      "imports": "dnxcore50"
    }
  },


  "scripts": {
    "postcompile": [
      "dotnet pack --output C:\\LibraryPackages --no-build --configuration %compile:Configuration%",
      "robocopy bin\\Release C:\\LibraryPackages\\%project:Name% /E /MIR"
    ]
  },

  "buildOptions": {
    "xmlDoc": true,
    "copyToOutput": {
      "include": [
        "project.json"
      ]
    }
  },

  "packOptions": {
    "repository": {
      "type": "git",
      "url": "https://github.com/Temitope-A/Delfi"
    }
  }
}
